{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["src","tutorials"],
    "excludePattern": "(node_modules/|docs)",
    "excludePattern": "(^|\\/|\\\\/)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "staticFiles": {
        "paths": ["./tutorials/img"]
      }
    }
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./documentation/",
    "recurse": true,
    "tutorials": "./tutorials",
    "readme": "README.md"
  }
}

